pipeline {
  agent any

    stages {
      stage("Setup"){
        steps {
          git branch: 'main', url: 'https://github.com/KauanButura99/Ebac-Teste-API-Automatizado.git'
          sh 'npm install'
        }
      }

      stage("Test"){
        steps {
          sh 'NO_COLOR=1 npm run cy:run-ci'
        }
      }
    }

    
}